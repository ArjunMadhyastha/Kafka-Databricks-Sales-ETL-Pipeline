{
  "datasets": [
    {
      "name": "dataset1",
      "displayName": "sales_transformed_mv",
      "queryLines": [
        "Select * from sales_transformed_mv;"
      ]
    },
    {
      "name": "efbf8bb3",
      "displayName": "Customers",
      "queryLines": [
        "SELECT sum(customer_per_order) from sales_transformed_mv where date_diff(to_date(timestamp_standard),current_date())=0;"
      ]
    },
    {
      "name": "e035c49c",
      "displayName": "Sales",
      "queryLines": [
        "select sum(amount) from sales_transformed_mv where date_diff(to_date(timestamp_standard),current_date())=0;"
      ]
    },
    {
      "name": "28fdb8e2",
      "displayName": "Orders",
      "queryLines": [
        "SELECT count(order_id) from sales_transformed_mv where date_diff(to_date(timestamp_standard),current_date())=0;"
      ]
    },
    {
      "name": "82f35833",
      "displayName": "Sales by Item",
      "queryLines": [
        "SELECT sum(Masala_Dosa_Q*Masala_Dosa_P) as Masala_Dosa, sum(Idli_Q*Idli_P) as Idli,sum(Idli_Vada_Q*Idli_Vada_P) as Idli_Vada, sum(Poori_Q*Poori_P) as Poori, sum(Chapati_Q*Chapati_P) as Chapati, sum(Plain_Dosa_Q*Plain_Dosa_P) as Plain_Dosa, sum(Tea_Q*Tea_P) as Tea, sum(Coffee_Q*Coffee_P) as Coffee, sum(Cold_Drink_Q*Cold_Drink_P) as Cold_Drink, sum(Lassi_Q*Lassi_P) as Lassi from sales_transformed_mv  where date_diff(to_date(timestamp_standard),current_date())=0;"
      ]
    },
    {
      "name": "dfab0c43",
      "displayName": "Quantity per Item",
      "queryLines": [
        "SELECT sum(Masala_Dosa_Q) as Masala_Dosa, sum(Idli_Q) as Idli,sum(Idli_Vada_Q) as Idli_Vada, sum(Poori_Q) as Poori, sum(Chapati_Q) as Chapati, sum(Plain_Dosa_Q) as Plain_Dosa, sum(Tea_Q) as Tea, sum(Coffee_Q) as Coffee, sum(Cold_Drink_Q) as Cold_Drink, sum(Lassi_Q) as Lassi from sales_transformed_mv  where date_diff(to_date(timestamp_standard),current_date())=0;"
      ]
    },
    {
      "name": "bec5be6f",
      "displayName": "Payment Mode",
      "queryLines": [
        "SELECT t.payment_mode, ROUND(t.cnt*100.0/t.total,2) as p from (SELECT payment_mode, count(payment_mode) as cnt, sum(count(payment_mode)) over () as total FROM sales_transformed_mv group by payment_mode) as t;"
      ]
    }
  ],
